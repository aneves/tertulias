<h2><%= pluralize @users.count, User.model_name.human %>:</h2>
<table>
	<thead><tr>
		<th><%= User.human_attribute_name :name %></th>
		<th><%= User.human_attribute_name :email %></th>
		<th><%= User.human_attribute_name :admin? %></th>
		<th />
	</tr></thead>
	<tbody>
	<% @users.each do |user| %>
		<tr>
			<td><%= user.name %></td>
			<td><%= user.email %></td>
			<td<%= " class='highlight'" if user.admin? %>><%= t user.admin?.to_s %></td>
		</tr>
	<% end %>
	</tbody>
</table>

<%#section class="collapsable collapsed">
  <h3><%= t 'tert.add_x', :model => Contribution.model_name.human% ></h3>
  <%= render :partial => "contributions/mini_form" % >
</section%>